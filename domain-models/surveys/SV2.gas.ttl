            ] ;
    gas:lineAzimuth [
        qudt:numericValue "150"^^xsd:double ;
        qudt:unit unit:DEG ;
            ] ;
    gas:tieAzimuth [
        qudt:numericValue "60"^^xsd:double ;
        qudt:unit unit:DEG ;
            ] ;
    gas:minStationSpacing [
